{
    "name": "ahmyi/cakephp_discord_log",
    "description": "Logging to Discord plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.5.0 || ^9.3"
    },
    "autoload": {
        "psr-4": {
            "Ahmyi\\DiscordLog\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ahmyi\\DiscordLog\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
